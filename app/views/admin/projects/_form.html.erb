<%= semantic_form_for [:admin, @project] do |f| %>

<%= f.inputs :name => "Basic" do %>
  <%= f.input :job_title,  :as => :select,  :collection => ["Development", "Digital Production", "Production"] %>
  <%= f.input :title %>
	<%= f.input :poster,  :as => :file %>
	<%= f.input :url, :as => :url %>
	<%= f.input :description, :as => :text %>
  
<!-- f.input :tag_list, :label => "Tags" -->

<% end %>



<%= f.actions %>
<% end %>
  
